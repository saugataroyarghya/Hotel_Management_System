<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hms.login">
    <children>
        <Text layoutX="192.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Receptionist LogIn Page">
            <font>
                <Font name="Gabriola" size="28.0" />
            </font>
        </Text>
        <TextField fx:id="username" layoutX="225.0" layoutY="148.0" onAction="#go_man_pan" promptText="username" />
        <PasswordField fx:id="password" layoutX="225.0" layoutY="200.0" onAction="#go_man_pan" promptText="password" />
        <Text layoutX="132.0" layoutY="166.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" textAlignment="RIGHT">
            <font>
                <Font name="Gabriola" size="19.0" />
            </font>
        </Text>
        <Text layoutX="140.0" layoutY="217.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" textAlignment="RIGHT">
            <font>
                <Font name="Gabriola" size="18.0" />
            </font>
        </Text>
        <Button fx:id="mybutton" layoutX="250.0" layoutY="284.0" mnemonicParsing="false" onAction="#go_rec_pan" prefHeight="40.0" prefWidth="99.0" style="-fx-background-radius: 15px;" text="Log In">
            <font>
                <Font name="Verdana" size="13.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
